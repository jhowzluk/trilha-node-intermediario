<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>usuarios.ejs</title>
</head>

<body>
    <%- include('partials/header') %>

        <h2>
            <%= title %>
        </h2>
        <ul>
            <% usuarios.forEach(usuario=> { %>
                <li><strong>
                        <%= usuario.nome %>
                    </strong> - <%= usuario.email %>
                </li>
                <% }) %>
        </ul>

        <%- include('partials/footer') %>

</body>

</html>